PORT=3000
NODE_ENV=development

# PostgreSQL — individual connection parameters
DB_HOST=localhost
DB_PORT=5432
DB_NAME=hotel_db
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# JWT
JWT_SECRET=change_me_to_a_long_random_string
JWT_EXPIRES_IN=7d

# Roles — include in signup body as "adminSecret" to register as admin
ADMIN_SECRET=change_me_to_a_secret_string

# Logging
LOG_LEVEL=info

# Seeding
# LISTINGS_SEED_COUNT=20    # number of fake listings to insert
